<!DOCTYPE html>
<html>
  <head  prefix="og: http://ogp.me/ns#"  >
    <%= display_meta_tags(default_meta_tags) %>
    <title><%= full_title(yield(:title)) %></title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', onload: "this.onload=null;this.rel='stylesheet'", rel: 'preload', as: 'style'  %>
    <script src="/javascript/swiper-bundle.min.js"></script>
    <%= javascript_pack_tag 'application' %>
    <%= render 'shared/shim' %>
    <%= render 'shared/Google_Analytics' %>
    <meta name="google-site-verification" content="o6POgoxu6qF1OuoCCm8P-LWvyJYNGAaYPnncoey0ghk" />
  </head>
  <body>
    <div id="wrapper">
      <%= yield %>
      <%= render 'shared/footer' %>
    </div>
  </body>
</html>